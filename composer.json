{
    "name": "yedrick/master",
    "description": "Paquete maestro para Yerson.",
    "version": "1.0.0",
    "keywords": ["laravel", "yerson", "yedrick"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yerson Jarandilla",
            "email": "yerfe717@gmail.com"
        }
    ],
    "require": {
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.8",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "classmap": [
            "src/database/migrations",
            "src/database/seeders",
            "src/app",
            "src/app/Console",
            "src/app/Exports",
            "src/app/Http",
            "src/app/Http/Controllers",
            "src/app/Listeners",
            "src/app/Models",
            "src/app/Providers",
            "src/app/Traits",
            "src/app/View",
            "src/assets",
            "src/config",
            "src/lang",
            "src/routes",
            "src/views"
        ],
        "psr-4": {
            "Yedrick\\Master\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true
    
